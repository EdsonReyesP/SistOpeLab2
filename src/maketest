OBJS	= main.o funciones.o lecturaImg.o convertirBN.o
SOURCE	= main.c funciones.c lecturaImg.c convertirBN.c
HEADER	= funciones.h
OUT	= salida; Lectura; BlancoNegro
CC	 = gcc
FLAGS	 = -g -c -Wall
LFLAGS	 = 

all: $(OBJS)
	$(CC) -g $(OBJS) -o $(OUT) $(LFLAGS)

main.o: main.c
	$(CC) $(FLAGS) main.c

funciones.o: funciones.c
	$(CC) $(FLAGS) funciones.c

lecturaImg.o: lecturaImg.c
	$(CC) $(FLAGS) lecturaImg.c

convertirBN.o: convertirBN.c
	$(CC) $(FLAGS) convertirBN.c


clean:
	